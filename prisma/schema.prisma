datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Response {
  id         String   @id @default(uuid())
  name       String
  department String
  scores     Json
  feedback   String?
  createdAt  DateTime @default(now())
}